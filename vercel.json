{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": "api/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/index.py",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    }
  ]
}
